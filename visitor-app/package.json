{
  "name": "datapower",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "./react-backend/index.js",
  "scripts": {
    "start": "node ./react-backend/index.js",
    "startmon": "nodemon ./react-backend/index.js",
    "postinstall": "cd react-backend/client && npm install",
    "heroku-postbuild":
      "cd react-backend/client && npm install --only=dev && npm install && npm run build",
    "build.client": "cd react-backend/client && npm run build",
    "test": "NODE_ENV=test tape '**/__tests__/*.test.js' | tap-spec",
    "start.client": "cd react-backend/client && npm run start",
    "cover": "nyc npm test",
    "cover.report": "nyc report --reporter=html",
    "cover.report.ci": "nyc report --reporter=text-lcov > coverage.lcov",
    "lint": "eslint .",
    "lint.fix": "eslint . --fix",
    "psql.build": "node ./db/scripts/index.js build",
    "psql.destroy": "node ./db/scripts/index.js destroy",
    "psql.empty": "node ./db/scripts/index.js empty",
    "psql.populate": "node ./db/scripts/index.js populate",
    "psql.migrate": "node ./db/scripts/index.js migrate",
    "psql.refresh": "node ./db/scripts/index.js refresh"
  },
  "repository": {
    "type": "git",
